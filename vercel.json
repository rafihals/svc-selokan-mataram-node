{
    "version": 2,
    "builds": [
        {
            "src": "api/image.js",
            "use": "@vercel/node"
        },
        {
            "src": "assets/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/image/(.*)",
            "dest": "/api/image.js?filename=$1"
        },
        {
            "src": "/assets/(.*)",
            "dest": "/assets/$1"
        }
    ]
}